SELECT count(DISTINCT NAME) from ANIMAL_INS\
\
#union \'bb\'e7\'bf\'eb\'c7\'cf\'b1\'e2\
SELECT ANIMAL_TYPE, COUNT(*) FROM ANIMAL_INS WHERE ANIMAL_TYPE = 'Cat'\
