group by NAME\
Having COUNT(*)>1\
order by NAME \
\
#HOUR \'c7\'d4\'bc\'f6 \'bb\'e7\'bf\'eb\'c7\'cf\'b1\'e2\
SELECT HOUR(DATETIME) as HOUR,count(*) as COUNT  FROM ANIMAL_OUTS  \
where HOUR(DATETIME) > 8 and HOUR(DATETIME) < 20 \
GROUP BY HOUR ORDER BY HOUR\
\
